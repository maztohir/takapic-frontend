    "husky": "^0.14.3",
    "lint-staged": "^4.1.3",
    "react-scripts": "1.0.13"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "format":
      "prettier --single-quote --trailing-comma='es5' --write \"*.js\" \"src/**/*.js\"",
    "format:watch":
      "chokidar 'src/**/*.js' -c \"test '{event}' = 'change' && prettier --single-quote --trailing-comma='es5' --write '{path}'\"",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,json,css}": ["prettier --write", "git add"]
  }
}
